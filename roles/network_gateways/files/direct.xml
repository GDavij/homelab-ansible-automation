<?xml version="1.0" encoding="utf-8"?>
<direct>
  <rule ipv="ipv4" table="filter" chain="OUTPUT" priority="0">-p tcp --dport 25 -j REJECT</rule>
  <rule ipv="ipv4" table="filter" chain="OUTPUT" priority="0">-p tcp -m multiport --dports 135,137,138,139,445 -j REJECT</rule>
  <rule ipv="ipv4" table="filter" chain="OUTPUT" priority="0">-p udp -m multiport --dports 135,137,138,139,445 -j REJECT</rule>
  <rule ipv="ipv4" table="filter" chain="OUTPUT" priority="0">-m state --state INVALID -j DROP</rule>
</direct>